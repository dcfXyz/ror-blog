<div class="row">
  <div class="col-lg-12">
    <div class="page-header">
      <h1>User List</h1>
    </div>
    <table class="table table-hover table-striped">
      <thead>
        <tr>
          <th>user_id</th>
          <th>username</th>
        </tr>
      </thead>
      <tbody>
      <% @users.each do |user| %>
        <tr>
          <td><%= user.id %></td>
          <td>
            <%= link_to user.username, user_path(user) %>
          </td>
        </tr>
      <% end %>
      </tbody>
    </table>
  </div>
</div>
